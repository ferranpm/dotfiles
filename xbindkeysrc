"urxvtc"
	Alt + Return

"urxvtc -e vim"
	Mod4 + v

"urxvtc -e bpython"
	Mod4 + p

"urxvtc -e irb"
	Mod4 + r

"urxvtc -e python3 /usr/bin/ranger"
	Mod4 + f

"dmenu_run -fn DejaVu"
	Alt + d

"firefox"
	Mod4 + i

"thunderbird"
	Mod4 + m

"setxkbmap es"
	Mod4 + e

"setxkbmap us"
	Mod4 + u

"xmodmap ~/dotfiles/Xmodmap"
	Mod4 + c

"urxvtc -title exit -geometry 40x20+0+400 -e ~/dotfiles/exit"
	Control + Alt + e

"amixer set Master 1%+"
	Mod4 + Up

"amixer set Master 1%-"
	Mod4 + Down

"import ~/screenshot.png"
	Print
