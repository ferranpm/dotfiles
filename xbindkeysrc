	Alt + d

"firefox"
	Mod4 + i

"setxkbmap es"
	Mod4 + e

"setxkbmap us"
	Mod4 + u

"xmodmap ~/dotfiles/Xmodmap"
	Mod4 + c

"~/dotfiles/exit"
	Alt + Shift + e

"amixer set Master 1%+"
	Mod4 + Up

