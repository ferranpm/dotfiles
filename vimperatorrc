" vim: set ft=vim:

noremap J :tabprevious<Return>
noremap K :tabnext<Return>
noremap <C-S-j> :tabmove -1<Return>
noremap <C-S-k> :tabmove +1<Return>
noremap gn :tabnew<Return>
noremap x :tabclose<Return>

cnoremap <C-p> <Up>
cnoremap <C-n> <Down>

noremap ; :
noremap : ;

noremap d <C-d>
noremap u <C-u>

noremap j 5j
noremap k 5k

